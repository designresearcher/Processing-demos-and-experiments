void setup()
{
  size(600,600);
}

void draw()
{
  
  if (keyPressed == true && key == 'a')
  {
          fill(#00BFFF);
         } else fill(#FFFFFF);
  rect(50,50,100,100);
  
   
   if (keyPressed == true && key == 'w')
  {
  fill(#00BFFF);
  } else fill(#FFFFFF);
  rect(155,50,100,100);
  
   
   if (keyPressed == true && key == 's')
  {
  fill(#00BFFF);
  } else fill(#FFFFFF);
  rect(50,155,100,100);
  
  
  if (keyPressed == true && key == 'x')
  {
  fill(#00BFFF);
  } else fill(#FFFFFF);
  rect(155,155,100,100);
  

}
